{
  "browserslist": {
    "production": "defaults",
    "development": [
      "extends browserslist-config-baseline"
    ]
  },
  "dependencies": {
    "big.js": "7.0.1",
    "bootstrap": "5.3.8",
    "immer": "10.2.0",
    "localforage": "1.10.0",
    "motion": "12.23.24",
    "papaparse": "5.5.3",
    "react": "19.2.0",
    "react-bootstrap": "2.10.10",
    "react-bootstrap-typeahead": "6.4.1",
    "react-currency-input-field": "4.0.3",
    "react-dom": "19.2.0",
    "react-hotkeys-hook": "5.2.1",
    "react-icons": "5.5.0",
    "recharts": "3.3.0",
    "use-immer": "0.11.0",
    "use-undo": "1.1.1",
    "wouter": "3.7.1"
  },
  "devDependencies": {
    "@axe-core/playwright": "4.11.0",
    "@biomejs/biome": "2.3.5",
    "@faker-js/faker": "10.1.0",
    "@playwright/test": "1.56.1",
    "@testing-library/dom": "10.4.1",
    "@testing-library/jest-dom": "6.9.1",
    "@testing-library/react": "16.3.0",
    "@testing-library/user-event": "14.6.1",
    "@types/big.js": "6.2.2",
    "@types/jest": "30.0.0",
    "@types/node": "24.10.1",
    "@types/papaparse": "5.5.0",
    "@types/react": "19.2.4",
    "@types/react-dom": "19.2.3",
    "@vitejs/plugin-react": "5.1.1",
    "@vitest/coverage-v8": "4.0.9",
    "@vitest/ui": "4.0.9",
    "autoprefixer": "10.4.22",
    "browserslist-config-baseline": "0.5.0",
    "console-fail-test": "0.5.0",
    "happy-dom": "20.0.10",
    "typescript": "5.9.3",
    "vite": "7.2.2",
    "vite-plugin-pwa": "1.1.0",
    "vite-plugin-sri3": "1.1.0",
    "vitest": "4.0.9",
    "vitest-when": "0.10.0",
    "workbox-window": "7.3.0"
  },
  "name": "guitos",
  "pnpm": {
    "onlyBuiltDependencies": [
      "@biomejs/biome",
      "@swc/core",
      "esbuild"
    ],
    "minimumReleaseAge": "2880",
    "overrides": {
      "vite": "npm:rolldown-vite@7.1.19"
    }
  },
  "private": true,
  "scripts": {
    "bump:playwright-image": "PLAYWRIGHT_VERSION=$(pnpm ls @playwright/test --json --depth=0 | jq -r '.[0].devDependencies[\"@playwright/test\"].version') && sed -i -E \"s|(mcr\\.microsoft\\.com/playwright:)v[0-9]+\\.[0-9]+\\.[0-9]+(@sha256:[a-f0-9]+)? +# ratchet:mcr\\.microsoft\\.com/playwright:v[0-9]+\\.[0-9]+\\.[0-9]+|\\1v$PLAYWRIGHT_VERSION\\2 # ratchet:mcr.microsoft.com/playwright:v$PLAYWRIGHT_VERSION|\" .github/workflows/*.yml",
    "bump:biome-schema": "pnpx @biomejs/biome migrate --write",
    "build": "vite build",
    "bundle": "pnpm dlx vite-bundle-visualizer",
    "coverage": "vitest run --coverage --silent --exclude 'e2e'",
    "coverage:ui": "vitest --ui --open --coverage --silent --exclude 'e2e'",
    "lint": "biome check --fix",
    "lint:packages": "pnpm dedupe",
    "schema": "pnpm dlx generate-schema ./docs/guitos-sample.json",
    "serve": "vite preview",
    "start": "vite",
    "test": "pnpm test:unit --run && pnpm test:e2e",
    "test:unit": "vitest --typecheck --exclude 'e2e'",
    "test:e2e": "playwright test --reporter=list --project chromium",
    "test:e2e:mobile": "playwright test --reporter=list --project 'Mobile Chrome'",
    "test:e2e:mobile:ui": "playwright test --ui --project 'Mobile Chrome'",
    "test:e2e:ui": "playwright test --ui --project chromium",
    "knip": "pnpm dlx knip"
  },
  "type": "module",
  "version": "1.6.2"
}
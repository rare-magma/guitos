    "e2e:ui": "playwright test --ui --project chromium",
    "lint": "biome check --fix",
    "lint:packages": "pnpm dedupe",
    "schema": "pnpm dlx generate-schema ./docs/guitos-sample.json",
    "serve": "vite preview",
    "start": "vite",
    "test": "vitest --typecheck --exclude 'e2e'"

{
  "name": "guitos",
  "version": "0.31.0",
  "private": true,
  "type": "module",
  "scripts": {
    "start": "vite",
    "build": "vite build",
    "serve": "vite preview",
    "test": "vitest --exclude 'e2e'",
    "e2e": "playwright test --reporter=list --project chromium",
    "e2e:ui": "playwright test --ui --project chromium",
    "e2e:mobile": "playwright test --reporter=list --project 'Mobile Chrome'",
    "e2e:mobile:ui": "playwright test --ui --project 'Mobile Chrome'",
    "coverage": "vitest run --coverage --silent",
    "coverage:ui": "vitest --ui --open --coverage --silent",
    "lint": "pnpm dlx eslint src e2e --fix && pnpm dlx stylelint src/**/*.css --fix && pnpm dlx prettier --write src e2e",
    "bundle": "pnpm dlx vite-bundle-visualizer"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": "defaults",
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "big.js": "6.2.1",
    "bootstrap": "5.3.2",
    "immer": "10.0.3",
    "localforage": "1.10.0",
    "papaparse": "5.4.1",
    "react": "18.2.0",
    "react-bootstrap": "2.9.2",
    "react-bootstrap-typeahead": "6.3.2",
    "react-currency-input-field": "3.8.0",
    "react-dom": "18.2.0",
    "react-hotkeys-hook": "4.5.0",
    "react-icons": "4.12.0",
    "react-router-dom": "6.20.1",
    "recharts": "2.12.0",
    "use-immer": "0.9.0",
    "use-undo": "1.1.1"
  },
  "devDependencies": {
    "@playwright/test": "1.41.2",
    "@semantic-release/changelog": "6.0.3",
    "@semantic-release/git": "10.0.1",
    "@simbathesailor/use-what-changed": "2.0.0",
    "@testing-library/jest-dom": "6.1.4",
    "@testing-library/react": "14.1.2",
    "@testing-library/user-event": "14.5.2",
    "@types/big.js": "6.2.2",
    "@types/jest": "29.5.8",
    "@types/node": "20.11.16",
    "@types/papaparse": "5.3.14",
    "@types/react": "18.2.61",
    "@types/react-dom": "18.2.18",
    "@types/react-table": "7.7.18",
    "@typescript-eslint/eslint-plugin": "6.11.0",
    "@typescript-eslint/parser": "6.17.0",
    "@vitejs/plugin-react": "4.2.0",
    "@vitest/coverage-v8": "1.0.2",
    "@vitest/ui": "1.1.1",
    "autoprefixer": "10.4.18",
    "eslint": "8.55.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-config-react-app": "7.0.1",
    "eslint-plugin-jest-dom": "5.1.0",
    "eslint-plugin-jsx-a11y": "6.8.0",
    "eslint-plugin-prettier": "5.0.1",
    "eslint-plugin-react": "7.33.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-react-refresh": "0.4.5",
    "eslint-plugin-testing-library": "6.1.2",
    "jsdom": "22.1.0",
    "prettier": "3.2.4",
    "prettier-plugin-organize-imports": "3.2.4",
    "semantic-release": "22.0.10",
    "stylelint": "15.11.0",
    "stylelint-config-standard": "34.0.0",
    "stylelint-prettier": "4.1.0",
    "typescript": "5.2.2",
    "vite": "5.0.12",
    "vite-plugin-eslint": "1.8.1",
    "vite-plugin-pwa": "0.18.1",
    "vite-plugin-sri3": "1.0.3",
    "vitest": "1.1.0",
    "workbox-window": "7.0.0"
  }
}
